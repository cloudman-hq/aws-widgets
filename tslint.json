{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-eslint-rules",
        "tslint-config-prettier",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "jsx-no-lambda": false,
        "import-name": false,
        "no-boolean-literal-compare": false,
        "jsx-boolean-value": false,
        "variable-name": false
    },
    "linterOptions": {
        "exclude": [
            "build",
            "node_modules",
            "src/**/*.stories.tsx"
        ]
    },
    "rulesDirectory": []
}