{
  "key": "com.aws.widget.confluence-addon",
  "name": "AWS Widgets",
  "description": "Live document for your AWS resources.",
  "vendor": {
    "name": "ZenUML",
    "url": "https://www.zenuml.com/"
  },
  "baseUrl": "https://pensive-murdock-1f6fa1.netlify.com/",
  "enableLicensing": true,
  "links": {
    "self": "/atlassian-connect.json",
    "homepage": "https://www.zenuml.com"
  },
  "authentication": {
    "type": "none"
  },
  "scopes": [
    "READ",
    "WRITE"
  ],
  "modules": {
    "dynamicContentMacros": [
      {
        "key": "aws-widget-macro",
        "name": {
          "value": "AWS Widget"
        },
        "url": "/index.html?pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}",
        "description": {
          "value": "Create a ZenUML Sequence Diagram"
        },
        "outputType": "block",
        "bodyType": "none",
        "icon": {
          "width": 16,
          "height": 16,
          "url": "logo192.png"
        },
        "documentation": {
          "url": "https://www.zenuml.com/ZenUML_Sequence_Diagram_addon_help.html"
        },
        "categories": [
          "visuals"
        ],
        "featured": true
      }
    ]
  },
  "apiMigrations": {
    "gdpr": true
  }
}
