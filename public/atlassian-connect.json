{
  "key": "com.aws.widget.confluence-addon",
  "name": "AWS Widgets",
  "description": "Live document for your AWS resources.",
  "vendor": {
    "name": "ZenUML",
    "url": "https://www.zenuml.com/"
  },
  "baseUrl": "https://awswidgets.zenuml.com/",
  "enableLicensing": false,
  "links": {
    "self": "/atlassian-connect.json",
    "homepage": "https://www.zenuml.com"
  },
  "authentication": {
    "type": "none"
  },
  "scopes": [
    "READ",
    "WRITE"
  ],
  "modules": {
    "dynamicContentMacros": [
      {
        "key": "aws-widget-macro",
        "name": {
          "value": "AWS Widget"
        },
        "url": "/index.html?pageId={page.id}&pageVersion={page.version}&macroId={macro.id}&uuid={uuid}",
        "description": {
          "value": "Put the live status of your resource on the page."
        },
        "outputType": "block",
        "bodyType": "none",
        "editor": {
          "url": "/editor",
          "editTitle": {
            "value": "Update"
          },
          "insertTitle": {
            "value": "Add an AWS Resource to the page"
          },
          "width": "1240px",
          "height": "500px"
        },
        "icon": {
          "width": 192,
          "height": 192,
          "url": "logo192.png"
        },
        "documentation": {
          "url": "https://www.zenuml.com/ZenUML_Sequence_Diagram_addon_help.html"
        },
        "categories": [
          "visuals"
        ],
        "featured": true
      }
    ],
    "configurePage": {
      "url": "/settings",
      "name": {
        "value": "AWS Widgets Settings"
      },
      "key": "aws-widgets-settings-configure-page"
    }
  },
  "apiMigrations": {
    "gdpr": true
  }
}
